# Test environment credentials
# Copy to test.enc.env and encrypt with: sops -e -i credentials/test.enc.env
#
# Each environment has its own OVH Public Cloud project.

# OVH project
TF_VAR_service_name=your_test_project_id

# OpenStack
OS_AUTH_URL=https://auth.cloud.ovh.net/v3
OS_IDENTITY_API_VERSION=3
OS_USER_DOMAIN_NAME=Default
OS_PROJECT_DOMAIN_NAME=Default
OS_TENANT_ID=your_test_project_id
OS_TENANT_NAME=your_test_tenant_name
OS_USERNAME=user-openstack-test
OS_PASSWORD=your_openstack_password
OS_REGION_NAME=GRA11
