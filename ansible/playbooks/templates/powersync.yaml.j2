    - type: postgresql
      uri: !env PS_DATA_SOURCE_URI
      sslmode: verify-ca

storage:
  type: postgresql
  uri: !env PS_STORAGE_URI
  sslmode: verify-ca

port: !env PS_PORT

sync_rules:
  path: /config/sync_rules.yaml

client_auth:
  jwks_uri: !env PS_JWKS_URL
  audience: ["powersync"]

api:
  tokens:
    - powersync-dev-token
